{
  "name": "MightyText No-Pro",
  "short_name": "MT No-Pro",
  "description": "Removes the 'Go Pro' button on Mighty Text web app",
  "version": "1.1",
  "manifest_version":2,
  "icons": { "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://mightytext.net/*"],
      "js": ["no-pro.js"]
    }
  ]
}